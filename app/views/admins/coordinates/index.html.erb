<div class = "admin_coordinate_index">
<div class="row">
  <div class="col-xs-1"></div>
  <div class="col-xs-10">
  	<h1 class = "font">管理者のコーディネート一覧</h1>
  </div>
  <div class="col-xs-1"></div>
</div>


<div class="row">
  <div class="col-xs-1"></div>
  <div class="col-xs-10 admin_coordinate_index2">
	<% @coordinates.each do |coordinate| %>
		<%= link_to admins_coordinate_path(coordinate.id) do %>
			<%= attachment_image_tag coordinate, :coordinate_image, :fill, 150, 150, format: 'jpeg' %>
		<% end %>
	<% end %>
  </div>
  <div class="col-xs-1"></div>
</div>
</div>