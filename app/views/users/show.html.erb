<h1>ユーザー詳細</h1>
<%= attachment_image_tag @user, :profile_image, :fill, 200, 200, format: 'jpeg', fallback: "no_image.jpg", size:'200x200', class: 'img-circle profile-thumb my-img edit-img' %>
<%= @user.introduction %>

<% @coordinates.each do |coordinate| %>
	<%= link_to coordinate_path(coordinate.id) do %>
		<%= attachment_image_tag coordinate, :coordinate_image, :fill, 150, 150, format: 'jpeg' %>
	<% end %>
<% end %>