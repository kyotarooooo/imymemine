<div class="row field">
    <div class="col-xs-1"></div>
    <div class="col-xs-10">
    	<h1>ユーザー一覧</h1>
			<table class="table">
			  <thead>
			    <tr>
			      <th></th>
			      <th>ユーザー名</th>
			      <th>性別</th>
			      <th>身長</th>
			    </tr>
			  </thead>
			<% @users.each do |user| %>
			  <tbody>
			    <tr>
			      <th>
			      	<%= link_to user_path(user.id) do %>
			      	<%= attachment_image_tag user, :profile_image, :fill, 60, 60, format: 'jpeg', fallback: "no_image.jpg", size:'60x60', class: 'img-circle profile-thumb my-img edit-img' %>
			      	<% end %>
			      </th>
			      <td><%= user.user_name %></td>
			      <td><%= user.sex %></td>
			      <td><%= user.height %></td>
			    </tr>
			  </tbody>
			<% end %>
			</table>
    </div>
    <div class="col-xs-1"></div>
</div>